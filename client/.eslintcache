  <div className=\"header__searchIcon\">\n            <SearchOutlinedIcon style={{ fontSize: 30 }} />\n          </div>\n        )}\n\n        {auth ? (\n          <div>\n            <MailOutlineOutlinedIcon className={classes.authIcon} />\n            <NotificationsOutlinedIcon className={classes.authIcon} />\n            <AccountCircleOutlinedIcon className={classes.authIcon} />\n          </div>\n        ) : (\n          <div className=\"header__btn\">{ account() }</div>\n        )}\n      </Toolbar>\n    </AppBar>\n  );\n}\n",["65","66"],"/Users/hoa/Documents/project/devconnector/client/src/view/components/Banner/Banner.jsx",["67"],"/Users/hoa/Documents/project/devconnector/client/src/router.js",[],"/Users/hoa/Documents/project/devconnector/client/src/redux/store.js",[],"/Users/hoa/Documents/project/devconnector/client/src/redux/reducers/user.reducer.js",[],"/Users/hoa/Documents/project/devconnector/client/src/redux/constants/user.constants.js",[],"/Users/hoa/Documents/project/devconnector/client/src/redux/actions/user.action.js",[],"/Users/hoa/Documents/project/devconnector/client/src/view/pages/auth/SignIn/SignIn.jsx",[],"/Users/hoa/Documents/project/devconnector/client/src/view/pages/auth/SignUp/SignUp.jsx",[],"/Users/hoa/Documents/project/devconnector/client/src/utils/helper/index.js",[],"/Users/hoa/Documents/project/devconnector/client/src/utils/validation/index.js",[],"/Users/hoa/Documents/project/devconnector/client/src/utils/helper/regex.js",[],"/Users/hoa/Documents/project/devconnector/client/src/utils/helper/isEmtpy.js",[],"/Users/hoa/Documents/project/devconnector/client/src/redux/actions/index.action.js",[],{"ruleId":"68","severity":1,"message":"69","line":56,"column":16,"nodeType":"70","messageId":"71","endLine":56,"endColumn":23},{"ruleId":"72","replacedBy":"73"},{"ruleId":"74","replacedBy":"75"},{"ruleId":"68","severity":1,"message":"69","line":17,"column":16,"nodeType":"70","messageId":"71","endLine":17,"endColumn":23},"no-unused-vars","'setAuth'